<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<StackPane id="StackPane" fx:id="spApplication" prefHeight="330.0" prefWidth="330.0" style="-fx-background-color: TRANSPARENT;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.github.naoghuman.yin.yang.application.ApplicationPresenter">
    <children>
        <HBox fx:id="hbMenuButtons" alignment="CENTER" fillHeight="false" maxHeight="-Infinity" maxWidth="-Infinity" spacing="7.0" style="-fx-background-color: LIGHTBLUE;">
            <children>
                <Button fx:id="bShowOptionDialog" mnemonicParsing="false" onAction="#onActionShowOptionDialog" text="..." />
                <Button fx:id="bMinimizeApplication" mnemonicParsing="false" onAction="#onActionMinimizeApplication" text="_" />
                <Button fx:id="bCloseApplication" mnemonicParsing="false" onAction="#onActionCloseApplication" text="X" />
            </children>
            <padding>
                <Insets bottom="7.0" left="14.0" right="14.0" top="7.0" />
            </padding>
            <StackPane.margin>
                <Insets bottom="150.0" />
            </StackPane.margin>
        </HBox>
        <HBox fx:id="hbTaiChiTerms" alignment="CENTER" fillHeight="false" maxHeight="-Infinity" maxWidth="-Infinity" mouseTransparent="true" prefWidth="300.0" StackPane.alignment="CENTER">
            <children>
                <Label fx:id="lYangTerm" minHeight="39.0" text="&lt;yang&gt;">
                    <font>
                        <Font size="18.0" />
                    </font>
                </Label>
                <Label fx:id="lYinTerm" minHeight="39.0" text="&lt;yin&gt;">
                    <font>
                        <Font size="18.0" />
                    </font>
                </Label>
            </children>
        </HBox>
    </children>
</StackPane>
